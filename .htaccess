RewriteRule ^index$ index.php [L]
RewriteRule ^alumni$ alumni.php [L]
RewriteRule ^batchmates$ batchmates.php [L]
RewriteRule ^dashboard$ dashboard.php [L]
RewriteRule ^forum$ forum.php [L]
RewriteRule ^survey$ survey.php [L]
RewriteRule ^statistic$ statistic.php [L]
RewriteRule ^profile$ profile.php [L]
RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $.php

#Custom Error 403 
ErrorDocument 403 http://localhost/Tracer/ErrorPages/403.php
#Custom Error 404 
ErrorDocument 404 http://localhost/Tracer/ErrorPages/404.php
#Custom Error 405 
ErrorDocument 405 http://localhost/Tracer/ErrorPages/405.php
#Custom Error 500 
ErrorDocument 500 http://localhost/Tracer/ErrorPages/500.php
#Custom Error 503 
ErrorDocument 503 http://localhost/Tracer/ErrorPages/503.php


